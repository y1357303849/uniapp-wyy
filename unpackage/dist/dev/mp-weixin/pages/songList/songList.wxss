page {
  background-color: #541e24;
}
page .songListBox .songListTop {
  margin-top: 20rpx;
  white-space: nowrap;
  background-color: #5d3131;
}
page .songListBox .songListTop scroll-view {
  min-width: 100%;
  height: 50rpx;
}
page .songListBox .songListTop scroll-view .topItem {
  text-align: center;
  width: 200rpx;
  height: 100%;
  line-height: 50rpx;
  font-size: 40rpx;
  color: white;
  display: inline-block;
}
page .songListBox ::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}
page .songListBox .songLists {
  margin-top: 20rpx;
  background-color: #5d3131;
  display: flex;
  flex-wrap: wrap;
}
page .songListBox .songLists .songListsItem {
  width: 365rpx;
  height: 380rpx;
  margin: 10rpx auto;
  background-color: #5d3131;
}
page .songListBox .songLists .songListsItem image {
  width: 355rpx;
  margin: 5rpx;
  height: 280rpx;
  border-radius: 20rpx;
}
page .songListBox .songLists .songListsItem text {
  width: 100%;
  height: 100rpx;
  color: white;
}
page .songListBox .isOver {
  width: 100%;
  height: 50rpx;
  margin-top: 20rpx;
  text-align: center;
  font-size: 30rpx;
  font-weight: 900;
  color: white;
}

