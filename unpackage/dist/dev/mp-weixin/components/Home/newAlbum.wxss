.newAlbumBox {
  width: 750rpx;
  margin-top: 10rpx;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
  background-color: #5d3131;
  border-radius: 10rpx;
}
.newAlbumBox .albumItem {
  width: 355rpx;
  height: 200rpx;
  margin: 10rpx auto;
}
.newAlbumBox .albumItem image {
  width: 50%;
  height: 200rpx;
  float: left;
  border-radius: 20rpx;
}
.newAlbumBox .albumItem .albumName {
  width: 50%;
  height: 200rpx;
  float: left;
}
.newAlbumBox .albumItem .albumName text {
  width: 155rpx;
  height: 50%;
  font-size: 25rpx;
  text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  color: white;
  padding-left: 10rpx;
}

