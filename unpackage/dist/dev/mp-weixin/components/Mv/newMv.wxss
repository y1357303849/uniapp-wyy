.newMvBox {
  width: 750rpx;
  margin-top: 10rpx;
  background-color: #5d3131;
}
.newMvBox .newMvTop {
  width: 750rpx;
  height: 100rpx;
}
.newMvBox .newMvTop .titleName {
  width: 200rpx;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  font-size: 50rpx;
  color: white;
  float: left;
}
.newMvBox .newMvTop .tagList {
  float: left;
}
.newMvBox .newMvTop .tagList text {
  width: 100rpx;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  color: white;
  display: block;
  float: left;
}
.newMvBox .newMvs {
  width: 750rpx;
  margin-top: 10rpx;
  display: flex;
  flex-wrap: wrap;
}
.newMvBox .newMvs .newMvsItem {
  width: 365rpx;
  height: 380rpx;
  margin: 10rpx auto;
}
.newMvBox .newMvs .newMvsItem image {
  width: 355rpx;
  margin: 5rpx;
  height: 280rpx;
  border-radius: 20rpx;
}
.newMvBox .newMvs .newMvsItem text {
  width: 100%;
  height: 100rpx;
  color: white;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}

